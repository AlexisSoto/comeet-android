        android:layout_height="wrap_content"
        android:text="test lecture data"
        android:onClick="btn_test_read_data"

        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/btn_position" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_retour"
        android:text="Valeur retournÃ©e :"
        android:layout_alignBottom="@+id/include"
